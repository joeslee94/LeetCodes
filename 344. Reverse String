public class Solution
{
    public void ReverseString(char[] s)
    {
        char temp;
        if (s.Length % 2 != 0)
        {
            for (int i = 0; i < s.Length/2; i++)
            {
                temp = s[i];
                s[i] = s[s.Length - 1 - i];
                s[s.Length - 1 - i] = temp;
            }
        }
        else
        {
            for (int i = 0; i < s.Length / 2; i++)
            {
                temp = s[i];
                s[i] = s[s.Length - 1 - i];
                s[s.Length - 1 - i] = temp;
            }
        }
        
    }
}
